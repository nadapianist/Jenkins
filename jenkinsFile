pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                echo 'Pulling...';
                git branch: 'main',
                url :'https://github.com/nadapianist/Jenkins.git';
                   }
        }

        stage('Test maven') {
            steps {
                sh 'mvn -version'
                
            }
        }

       
    }
}
